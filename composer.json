{
    "name": "likewares/laravel-firewall",
    "description": "Web Application Firewall (WAF) package pour Laravel",
    "keywords": [
        "laravel",
        "firewall",
        "security",
        "waf",
        "blacklist",
        "xss",
        "sqli",
        "rfi",
        "lfi"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "KOUAOBILE ASSANDE HUGUES",
            "email": "assandehugues@gmail.com",
            "homepage": "https://crm.likewares.com",
            "role": "CEO Developer"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2|^8.3|^8.4",
        "laravel/framework": "^9.0|^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.9",
        "jenssegers/agent": "^2.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5|^10.0|^11.0|^12.0",
        "orchestra/testbench": "^7.4|^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Likewares\\Firewall\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Likewares\\Firewall\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Likewares\\Firewall\\Provider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
}
